{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", "build/**"],
      "inputs": ["src/**/*.*", "static/**/*.*", "vite.config.js", "svelte.config.js", "tsconfig.json", "postcss.config.cjs", "tailwind.config.cjs"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
